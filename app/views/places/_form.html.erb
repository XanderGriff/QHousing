<%= form_for @place do |f| %>
  <fieldset class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control" %>
  </fieldset>

  <fieldset class="form-group">
    <%= f.label :rent %>
    <%= f.text_field :rent, class: "form-control" %>
  </fieldset>

  <fieldset class="form-group">
    <%= f.label :bedrooms, 'Bedrooms' %>
    <%= f.text_field :bedrooms, class: "form-control" %>
  </fieldset>

  <%= f.submit 'Add!', class: 'btn btn-primary' %>
<% end %>